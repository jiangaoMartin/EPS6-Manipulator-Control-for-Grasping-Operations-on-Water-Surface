		<world_transform xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
	</static>

	<static name="Obstacle010" type="box" physics="submerged" buoyant="true">
		<dimensions xyz="1.0 1.0 1.0"/>  <!-- 1m wide and 1m high. -->
		<origin xyz="0.0 10.0 0.0" rpy="0.0 0.0 0.0"/>
