	<dynamic name="Cyl" type="cylinder" physics="submerged">
		<dimensions radius="0.02" height="0.8"/>
		<origin xyz="5.5 4.0 0.0" rpy="0.0 1.5708 1.5708"/>

		<inertia ixx="0.000006" iyy="0.000006" izz="0.000006" ixy="0.0" ixz="0.0" iyz="0.0"/>
		<hydrodynamics viscous_drag="0.1 0.1 0.1" quadratic_drag="0.1 0.1 0.1"/>

		
		<buoyancy density="1000.0" center="0.0 0.0 0.0"/>
		<material name="Plastic"/>
		<look name="cylinder"/>
		<world_transform xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
		<sensor name="odom_sensor" rate="10.0" type="odometry">
			<parent>Cyl</parent>
			<origin xyz="5.5 4.0 0.0" rpy="0.0 0.0 0.0"/>
