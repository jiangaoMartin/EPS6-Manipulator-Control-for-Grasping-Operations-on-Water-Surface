				<position>0.05</position>
				<velocity>0.01</velocity>
				<angle>0.1</angle>
				<angular_velocity>0.05</angular_velocity>
			</noise>
			<history samples="5"/>
			<ros_publisher topic="/cyl/odom" frame_id="world_ned" child_frame_id="Cyl"/>
		</sensor>

	</dynamic>


